histPercent = function(x, ...) {
H = hist(x, plot = FALSE)
H$density = with(H, 100 * density* diff(breaks)[1])
labs = paste(round(H$density), "%", sep="")
plot(H, freq = FALSE, labels = labs, ylim=c(0, 1.08*max(H$density)),...)
}
